<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>AddCustomerServlet</servlet-name>
    <servlet-class>view.AddCustomerServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddCustomerServlet</servlet-name>
    <url-pattern>/addCustomer</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddCustomerController</servlet-name>
    <servlet-class>view.AddCustomerController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddCustomerController</servlet-name>
    <url-pattern>/addCustomerController</url-pattern>
  </servlet-mapping>

    <servlet>
        <servlet-name>LookCustomer</servlet-name>
        <servlet-class>view.LookCustomerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LookCustomer</servlet-name>
        <url-pattern>/lookCustomer</url-pattern>
    </servlet-mapping>

  <servlet>
    <servlet-name>DeleteCustomer</servlet-name>
    <servlet-class>view.DeleteCustomer</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DeleteCustomer</servlet-name>
    <url-pattern>/deleteCustomer</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpdateCustomerUI</servlet-name>
    <servlet-class>view.UpdateCustomerUI</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UpdateCustomerUI</servlet-name>
    <url-pattern>/updateCustomerUI</url-pattern>
  </servlet-mapping>
</web-app>
